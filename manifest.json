{
	"manifest_version": 2,
	"name": "Zen",
	"description": "A relaxing Chrome tab extension.",
	"version": "1.0",

	"icons": {
		"16": "assets/img/icon.png",
		"48": "assets/img/icon.png",
		"128": "assets/img/icon.png"
	},

	"chrome_url_overrides": {
		"newtab": "index.html"
	},

	"permissions": [
		"tabs",
		"storage",
		"cookies",
		"http://*/*", 
		"https://*/*"
	],

	"options_page": "index.html"
}